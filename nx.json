{
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "affected": { "defaultBase": "main" },
  "npmScope": "swap",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "web": { "tags": [] },
    "web-e2e": { "tags": [], "implicitDependencies": ["web"] },
    "app": { "tags": ["type:app"] },
    "app-e2e": { "tags": [], "implicitDependencies": ["app"] },
    "swap-forms": { "tags": [] },
    "swap-layout": { "tags": [] },
    "server": { "tags": [] },
    "server-common": { "tags": [] },
    "server-users": { "tags": [] },
    "server-auth": { "tags": [] },
    "swap-storybook": { "tags": [] }
  }
}
